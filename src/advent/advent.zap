?L3:	EQUAL? W,W?HIM \?L4
	CALL COPY-TABLE-B,P-PRO-HIM-OBJS,OBJS,51
	JUMP ?L6
?L4:	EQUAL? W,W?HER \FALSE
	CALL COPY-TABLE-B,P-PRO-HER-OBJS,OBJS,51
?L6:	GETB OBJS,0 >CNT
