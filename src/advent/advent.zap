	.FUNCT V-HINT
	CALL RESPOND-TO-HINT-REQUEST? >STACK
	ZERO? STACK \TRUE
	PRINTR "I'm afraid I have no hints for your current situation."

	.FUNCT SHOW-HINT,N,P
	GET HINT-TEXT-TBL,N >STACK
	PRINT STACK
	CRLF
	GET HINT-PENALTY-TBL,N >P
	ZERO? P /FALSE
	SUB SCORE,P >SCORE
	LESS? LANTERN-POWER,30 /?L3
	GET HINT-EXTENSION-TBL,N >STACK
	ADD LANTERN-POWER,STACK >LANTERN-POWER
?L3:	PUT HINT-PENALTY-TBL,N,0
	RTRUE

	.FUNCT I-OFFER-HINT,MAX,P,C,T,I
	GET HINT-PENALTY-TBL,0 >MAX
	ZERO? MAX /FALSE
	SET 'I,1
?L4:	GET HINT-PENALTY-TBL,I >P
	ZERO? P /?L15
	CALL IN-HINT-LOCATION?,I >STACK
	ZERO? STACK \?L9
	PUT HINT-TIMER-TBL,I,0
	JUMP ?L15
?L9:	GET HINT-CONDITION-TBL,I >C
	ZERO? C /?L14
	CALL C >STACK
	ZERO? STACK /?L15
?L14:	GET HINT-TIMER-TBL,I >T
	ADD T,1 >STACK
	PUT HINT-TIMER-TBL,I,STACK
	GET HINT-PATIENCE-TBL,I >STACK
	EQUAL? T,STACK \?L15
	CALL OFFER-HINT,I
	RTRUE
?L15:	IGRTR? 'I,MAX \?L4
	RFALSE

	.FUNCT OFFER-HINT,N
	GET HINT-PROMPT-TBL,N >STACK
	PRINT STACK
	CALL YES? >STACK
	ZERO? STACK /FALSE
	PRINTI "I am prepared to offer you a hint, but it will cost you "
	GET HINT-PENALTY-TBL,N >STACK
	PRINTN STACK
	PRINTI " point"
	EQUAL? N,1 /?L3
	PRINTC 115
?L3:	PRINTI ". Do you want the hint?"
	CALL YES? >STACK
	ZERO? STACK /FALSE
	CRLF
	CALL SHOW-HINT,N >STACK
	RSTACK

	.FUNCT IN-HINT-LOCATION?,N,LOCS,MAX,I
	GET HINT-LOCATION-TBL,N >LOCS
	ZERO? LOCS /FALSE
	GET LOCS,0 >MAX
	SET 'I,1
?L4:	GET LOCS,I >STACK
	EQUAL? HERE,STACK /TRUE
	IGRTR? 'I,MAX \?L4
	RFALSE

	.FUNCT RESPOND-TO-HINT-REQUEST?,MAX,C,S,U,I
	GET HINT-PENALTY-TBL,0 >MAX
	ZERO? MAX /FALSE
	SET 'I,1
?L4:	CALL IN-HINT-LOCATION?,I >STACK
	ZERO? STACK /?L14
	GET HINT-CONDITION-TBL,I >C
	ZERO? C /?L9
	CALL C >STACK
	ZERO? STACK /?L14
?L9:	GET T?43,I >STACK
	ZERO? STACK \?L10
	ZERO? S \?L14
	SET 'S,I
	JUMP ?L14
?L10:	SET 'U,I
	JUMP ?L6
?L14:	IGRTR? 'I,MAX \?L4
?L6:	ZERO? U /?L16
	CALL OFFER-HINT,U
	RTRUE
?L16:	ZERO? S /FALSE
	CALL SHOW-HINT,S
	RTRUE

